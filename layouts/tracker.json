{
  "shared_item_grid": {
	"type": "array",
	"orientation": "horizontal",
    "margin": "0,0",
    "content": [
		{"type": "array",
        "orientation": "horizontal",
        "margin": "0,0",
        "content": [
          {
            "type": "itemgrid",
            "item_margin": "6,1",
            "h_alignment": "left",
			"item_size": "50",
            "rows": [
				[ "BWSquare", "ColoredSquares", "Stars", "StarSameColor", "Triangles" ],
				[ "Dots", "ColoredDots", "SoundDots", "Symmetry" ],
				[ "Shapers", "RotatedShapers", "NegativeShapers", "Eraser"]
            ]
          },
        ]},
		{"type": "array",
        "orientation": "horizontal",
        "margin": "0,0",
        "content": [
          {
            "type": "itemgrid",
            "item_margin": "6,1",
            "h_alignment": "left",
			"item_size": "50",
            "rows": [
				[ "Unrandomized", "Uncommon" ],
				[ "Vaults", "Hard" ],
				[ "Discarded", "UTM" ],
            ]
          },
        ]},
	]
  },
  "tracker_default": {
    "type": "container",
    "background": "#000000",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "left",
          "v_alignment": "stretch",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "top",
              "content": {
                "type": "layout",
                "key": "shared_item_grid"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "vertical",
                "compact": true
              }
            }
          ]
        },
        {
			"type": "map",
                "maps": [
                  "fullmap"
                ]
		},
      ]
    }
  },
  "tracker_horizontal": {
    "type": "container",
    "background": "#000000",
    "content": {
      "type": "dock",
      "dropshadow": true,
      "content": [
        {
          "type": "dock",
          "dock": "bottom",
          "content": [
            {
              "type": "group",
              "header": "Items",
              "dock": "left",
              "margin": "0,0,3,0",
              "content": {
                "type": "layout",
                "h_alignment": "center",
                "v_alignment": "center",
                "key": "shared_item_grid"
              }
            },
            {
              "type": "group",
              "header": "Pinned Locations",
              "content": {
                "type": "recentpins",
                "style": "wrap",
                "h_alignment": "stretch",
                "v_alignment": "stretch",
                "orientation": "horizontal",
                "compact": true
              }
            }
          ]
        },
        {
                "type": "map",
                "maps": [
                  "fullmap"
                ]
        }
      ]
    }
  }
}
